      "package": "com.travelhub.app",
      "compileSdk": 35,
      "targetSdk": 34,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1C2D41"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permite que TravelHub use la c√°mara para escanear el QR de abordaje."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ca145540-16f9-45e8-be79-e64541eced86"
      }
    }
  }
}